<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="generateDVF">

	<runtime>
		<python module="msml.ext.misc" function="GenerateDVF" />
	</runtime>

	<input>
        <!-- TODO logical and physical type -->
		<arg name="RefMesh" logical="Mesh"
			physical="VTK" />
	</input>

	<output>
		<arg name="DVF" logical="image" physical="file.vtk" />
	</output>

	<parameters>
      <arg name="DVFFilename" physical="str" optional="true">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
      </arg>
		<arg name="DeformedMesh" physical="VTK" />
		<arg name="multipleReferenceGrids" physical="bool" />
	</parameters>

</msml:operator>
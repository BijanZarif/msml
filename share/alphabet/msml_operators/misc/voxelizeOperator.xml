<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="voxelize">
    <runtime>
        <python module="msml.ext.misc" function="VoxelizeSurfaceMeshPython" />
    </runtime>

    <input>
        <arg name="surfaceMesh" logical="triangularMesh" physical="vtk" />
    </input>

    <output>
        <arg name="image" logical="segmentationImage" physical="vti" />
    </output>
    <parameters>
        <arg name="imageFilename" physical="string" optional="true">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
        </arg>
        <arg name="resolution" physical="int" optional="true" default="0">
            <meta key="doc">
                  either resolution or referenceCoordinateGrid must be set 
            </meta>
        </arg>
        <arg name="referenceCoordinateGrid" physical="vtk" optional="true" default="">
            <meta key="doc">
                  either resolution or referenceCoordinateGrid must be set 
            </meta>
        </arg>
        <arg name="disableFillHoles" physical="bool" optional="true" default="false">
        </arg>
    </parameters>
</msml:operator>
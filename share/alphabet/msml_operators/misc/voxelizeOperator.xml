<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="voxelize">
	<runtime>
		<python module="msml.ext.misc" function="voxelizeSurfaceMesh" />
	</runtime>

	<input>
		<arg name="surfaceMesh" type="triangularMesh" format="file.vtk" />
	</input>

	<output>
		<arg name="image" type="segmentationImage" format="file.vti" />
	</output>
	<parameters>
       <arg name="imageFilename" type="filename" optional="1">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
      </arg>
		<arg name="resolution" type="int" />
	</parameters>
</msml:operator>
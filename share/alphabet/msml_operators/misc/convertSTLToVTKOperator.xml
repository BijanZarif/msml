<?xml version="1.0" encoding="UTF-8"?>
<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="stl2vtk">

	<runtime>
		<python module="msml.ext.misc" function="convertSTLToVTK" />
	</runtime>

	<input>
		<arg name="STLMesh" type="triangularMesh" format="file.stl" />
	</input>

	<output>
		<arg name="VTKMesh" type="triangularMesh" format="file.vtk" />
	</output>
   
   <parameters>
      <arg name="vtkFilename" type="filename" optional="1">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
      </arg>
	</parameters>
</msml:operator>

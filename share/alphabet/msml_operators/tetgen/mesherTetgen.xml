<msml:operator xmlns:msml="http://sfb125.de/msml"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://sfb125.de/msml" name="mesherTetgen">

    <runtime>
        <python module="msml.ext.tetgen" function="createVolumeMeshTetgen"/>
    </runtime>

    <input>
        <arg name="surfaceMesh" logical="triangularMesh" physical="vtk"/>
    </input>

    <output>
        <arg name="mesh" logical="linearTetMesh" physical="vtk"/>
    </output>

    <parameters>
        <arg name="preserveBoundary" physical="bool"/>

        <arg name="meshFilename" physical="str" optional="true">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
        </arg>

    </parameters>
</msml:operator>
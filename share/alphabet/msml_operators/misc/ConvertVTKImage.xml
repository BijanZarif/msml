<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="ConvertVTKImage">
    <runtime>
        <python module="msml.ext.misc" function="ConvertVTKImage" />
    </runtime>

    <input>
        <arg name="inputImage" logical="image" physical="file.vtk" />
    </input>

    <output>
        <arg name="outputImage" logical="image" physical="file.vtk" />
    </output>
    <parameters>
        <arg name="targetImageFilename" physical="string" optional="true" target="true">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
        </arg>
    </parameters>
</msml:operator>
<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="splitMesh">
    <runtime>
        <python module="msml.ext.misc" function="splitMesh" />
    </runtime>

    <input>
        <arg name="mesh" logical="VolumeMesh" physical="file.vtk" />
    </input>  
	
    <parameters>	
		<arg name="splittedMesh" physical="str" logical="VolumeMesh"/>
		<arg name="group" physical="vector.int" >
            <meta key="doc">
				Vector of material id to include in output mesh
            </meta>
        </arg>
       </parameters>
	<output>
        <arg name="splittedMesh" logical="VolumeMesh" physical="file.vtk" />
    </output>
</msml:operator>
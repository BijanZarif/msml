<!-- This physical element represents the mass -->
<msml:element xmlns:msml="http://sfb125.de/msml"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://sfb125.de/msml" name="mass" category="material"
              quantity="single">


    <description>
        Set properties of the mass
    </description>

    <parameters>
        <arg name="name" physical="str">
            <meta key="doc">
                Name the mass element
            </meta>
        </arg>



        <arg name="massDensity" physical="float" default="1">
            <meta key="doc" value="Set the mass density"/>
        </arg>
    </parameters>
</msml:element>

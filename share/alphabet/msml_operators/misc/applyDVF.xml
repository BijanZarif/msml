<?xml version="1.0" encoding="UTF-8"?>
<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="apply-dvf">

	<runtime>
		<python module="msml.ext.misc" function="ApplyDVF" />
	</runtime>

	<input>
		<arg name="ApplyDVF" logical="Image3D" physical="VTK" />
	</input>

	<output>
		<arg name="outputDefImage" logical="Image3D" physical="VTI" />
	</output>


	<parameters>
		<arg name="dvf" physical="VTK" />
		<arg name="multipleDVF" physical="bool" />
		<arg name="reverseDirection" physical="bool" />
	</parameters>
</msml:operator>


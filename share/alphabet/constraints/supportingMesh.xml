<msml:element xmlns:msml="http://sfb125.de/msml"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://sfb125.de/msml" name="supportingMesh" category="constraint"
              quantity="single">
  <description>
        Add a secondary mesh that is embedded into the main mesh. This can be used to simulate a vessel tree or bones in a coarse main mesh.
  </description>
  
  <parameters>
    <arg name="youngModulus" physical="float" logical="Scalar"/>
    <arg name="poissonRatio" physical="float" logical="Scalar"/>
    <arg name="filename" physical="VTK" logical="TetrahedralVolume"/>
    <arg name="massDensity" physical="float" logical="Scalar"/>
  </parameters>
</msml:element>

<msml:operator xmlns:msml="http://sfb125.de/msml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://sfb125.de/msml" name="TransformMeshBarycentric">
	
	<runtime>
		<python module="msml.ext.misc" function="TransformMeshBarycentricPython" />
	</runtime>

	<input>
    <arg name="mesh" physical="file.vtk"/>
    <arg name="referenceGrid" logical="Mesh" physical="file.vtk" />
    <arg name="deformedGrid" physical="file.vtk"/>
	</input>

	<output>
		<arg name="out_surface" logical="Mesh" physical="file.vtk" />
	</output>


	<parameters>
    <arg name="out_surface_filename" physical="str" optional="true" />
    <arg name="multipleDeformedGrid" physical="bool"/>
	</parameters>

</msml:operator>
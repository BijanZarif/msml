<msml:operator xmlns:msml="http://sfb125.de/msml"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://sfb125.de/msml" name="mesherCGALs2v">
    <runtime>
        <python module="msml.ext.cgal" function="CreateVolumeMeshs2v"/>
    </runtime>

    <input>
        <arg name="surfaceMesh" logical="triangularMesh" physical="file.vtk"/>  
    </input>

    <output>
        <arg name="mesh" logical="linearTetMesh" physical="file.vtk"/>
    </output>

    <parameters>
        <arg name="meshFilename" physical="str" optional="true">
            <meta key="doc">
                Optional input filename for the output.
            </meta>
        </arg>
        <arg name="preserveFeatures" physical="bool"/>
        <arg name="facet_angle" physical="float"/>
        <arg name="facet_size" physical="float"/>
        <arg name="facet_distance" physical="float"/>
        <arg name="cell_radius_edge_ratio" physical="float"/>
        <arg name="cell_size" physical="float"/>
        <arg name="odt" physical="bool"/>
        <arg name="lloyd" physical="bool"/>
        <arg name="pertube" physical="bool"/>
        <arg name="exude" physical="bool"/>
    </parameters>
</msml:operator>
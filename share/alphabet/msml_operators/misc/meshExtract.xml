<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="surfaceExtract">

	<runtime>
		<python module="msml.ext.misc" function="ExtractAllSurfacesByMaterial" />
	</runtime>

	<input>
		<arg name="meshIn" logical="Mesh" physical="vtk" />
	</input>

	<output>
		<arg name="meshOut" logical="Mesh" physical="vtk" />
	</output>

	<parameters>
		<arg name="cud" physical="bool" />
	</parameters>
</msml:operator>
<msml:msml xmlns:msml="http://sfb125.de/msml"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://sfb125.de/msml">

    <variables>
        <var name="refVolumeMesh" value="IRACD_liver_5.vtk" logical="TriangularSurface" physical="file.vtk"/>
        <var name="redVerticesCount" value="100" />
        <var name="redMaxEdgeRadiusRatio" value="2.0" />

        <var name="outputReduceMesh" value="liverReduced1800.vtk"  physical="file.vtk"/>
        <var name="outputLinVolumeMesh" value="liverSmallVolume_maxEdge2.0.vtk"  physical="file.vtk"/>
        <var name="outputQuadVolumeMesh" value="liverQuadVolume_maxEdge2.0.vtk"  physical="file.vtk"/>
        <var name="outputProjection" value="liverProjectionVolume_maxEdge2.0.vtk"  physical="file.vtk"/>
    </variables>

   <workflow>
		<ACVDReduceSurfaceMesh id="FixedReduced" inputMeshFile="${refVolumeMesh}"
												 outputMeshFile="${outputReduceMesh}"
												 verticesCount="${redVerticesCount}" asciiOutput="true"/>
       <TetgenCreateVolumeMesh id="linearVolumeMesh" surfaceMesh="${FixedReduced}" maxEdgeRadiusRatio="${redMaxEdgeRadiusRatio}" targetMeshFilename="${outputLinVolumeMesh}"/>
       <ConvertLinearToQuadraticTetrahedralMesh id="quadVolumeMesh" linearMesh="${linearVolumeMesh}" outQuadMesh="${outputQuadVolumeMesh}" />
       <ProjectVolumeMeshDisplacement id="projection" inputMesh="${quadVolumeMesh}" outputMesh="${outputProjection}" referenceMesh="${refVolumeMesh}" />

  </workflow>
    <environment>
        <solver linearSolver="iterativeCG" processingUnit="CPU"
                timeIntegration="dynamicImplicitEuler"/>
        <simulation>
            <step name="initial" dt="0.05" iterations="200"/>
        </simulation>
    </environment>
</msml:msml>
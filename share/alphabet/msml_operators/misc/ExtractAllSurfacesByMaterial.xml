<msml:operator xmlns:msml="http://sfb125.de/msml"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://sfb125.de/msml" name="ExtractAllSurfacesByMaterial">

    <runtime>
        <python module="msml.ext.misc" function="ExtractAllSurfacesByMaterial"/>
    </runtime>

    <input>
        <arg name="meshIn" logical="linearTetMesh+triangularMesh" physical="file.vtk"/>
    </input>

    <output>
        <arg name="meshOut" logical="linearTetMesh+triangularMesh" physical="file.vtk"/>
    </output>

    <parameters>
        <arg name="meshOutFilename" physical="str" />
        <arg name="cut" physical="bool" optional="true" default="false" />
    </parameters>
</msml:operator>
<!-- This physical element represents the mass -->
<msml:element xmlns:msml="http://sfb125.de/msml"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://sfb125.de/msml" name="mass" category="material"
              quantity="single">


    <description>
        Mass of the object specified by its density
    </description>

    <parameters>

        <arg name="massDensity" physical="float" logical="Scalar" default="1">
            <meta key="doc" value="Set the mass density"/>
        </arg>
    </parameters>
</msml:element>

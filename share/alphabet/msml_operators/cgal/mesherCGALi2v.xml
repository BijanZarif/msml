<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="mesherCGALi2v">

	<runtime>
		<python module="msml.ext.cgal" function="CreateVolumeMeshi2v" />
	</runtime>
	<input>
		<arg name="image" logical="segmentationImage" physical="VTI" />
	</input>

	<output>
		<arg name="mesh" logical="linearTetMesh" physical="VTK" />
	</output>

	<parameters>
    <arg name="meshFilename" physical="str" optional="true" />
		<arg name="facet_angle" physical="float" />
		<arg name="facet_size" physical="float" />
		<arg name="facet_distance" physical="float" />
		<arg name="cell_radius_edge_ratio" physical="float" />
		<arg name="cell_size" physical="float" />
		<arg name="odt" physical="bool" optional="true" default="false"/>
		<arg name="lloyd" physical="bool" optional="true" default="false" />
		<arg name="pertube" physical="bool" optional="true" default="false" />
		<arg name="exude" physical="bool" optional="true" default="false" />
	</parameters>
</msml:operator>
<msml:element xmlns:msml="http://sfb125.de/msml"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://sfb125.de/msml" name="supportingMesh" category="constraint"
              quantity="single">
  <description>
        Add a secondary mesh to support to main mesh. Usable for vessels or bones.
  </description>
  
  <parameters>
    <arg name="youngModulus" physical="float" />
    <arg name="poissonRatio" physical="float" />
    <arg name="filename" physical="string"/>
    <arg name="massDensity" physical="float" />
  </parameters>
</msml:element>

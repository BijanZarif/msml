<msml:operator xmlns:msml="http://sfb125.de/msml"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://sfb125.de/msml" name="mvrBCdataProducer">

    <runtime>
        <python module="msml.ext.mvrBCdataProducer" function="BCdata_for_Hf3Sim_Producer"/>
    </runtime>

    <input>
        <arg name="volumeMeshFilename" logical="Mesh" physical="vtu"/>
	    <arg name="surfaceMeshFilename" logical="TriangularSurface" physical="vtp">
	       </arg>
	    <arg name="ringFilename" logical="object" physical="vtp">
	       </arg>
    </input>

    <output>
		<arg name="points" logical="object" physical="vector.float" />
		<arg name="displacements" logical="object" physical="vector.float" />
    </output>

    <parameters>
        <arg name="outputFilename" logical="object" physical="string">
	       </arg>
    </parameters>

</msml:operator>

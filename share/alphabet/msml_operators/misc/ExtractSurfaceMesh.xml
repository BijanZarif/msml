<?xml version="1.0" encoding="UTF-8"?>
<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="ExtractSurfaceMesh">

  <runtime>
    <python module="msml.ext.misc" function="ExtractSurfaceMeshPython" />
  </runtime>

  <input>
    <arg name="mesh" logical="volumeMesh" physical="file.vtk" />
  </input>
  <parameters>
    <arg name="surfaceMeshFilename" physical="str"  />
  </parameters>
  <output>
    <arg name="surfaceOut" logical="surface" physical="file.vtk"/>
  </output>

</msml:operator>


<?xml version="1.0" encoding="UTF-8"?>
<msml:operator xmlns:msml="http://sfb125.de/msml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sfb125.de/msml" name="SurfaceMeshNumberOfElements">
    <meta key="doc">
        Count surface elements
    </meta>
	<runtime>
		<python module="msml.ext.misc" function="SurfaceMeshNumberOfElements" />
	</runtime>

	<input>
		<arg name="mesh" logical="TriangularSurface" physical="file.vtk" />
	</input>

	<output>
		<arg name="numberOfElements" physical="int" />
	</output>

</msml:operator>

